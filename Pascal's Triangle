vector<vector<int>> pascalTriangle(int N) {
    vector<vector<int>> triangle; 
    for (int i = 0; i < N; i++) {     
        vector<int> row(i + 1, 1);     
        for (int j = 1; j < i; j++) {
            row[j] = triangle[i - 1][j - 1] + triangle[i - 1][j];
        }
        triangle.push_back(row);
    }
    return triangle;
}
